{
    "name": "solumdesignum/package-env-loader",
    "description": "Framework agnostic environment loader that abstracts PHP dotEnv for faster env loading.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskars Germovs",
            "email": "oskars_germovs@inbox.lv",
            "homepage": "https://solum-designum.eu"
        }
    ],
    "homepage": "https://github.com/solumdesignum/package-env-loader",
    "keywords": [
        "packageEnvLoader",
        "pacakge",
        "loader",
        "env",
        "dotenv",
        "environment"
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "vlucas/phpdotenv": "^v5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9|^10|^11",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5|^6|^7|^8|^9",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "SolumDeSignum\\PackageEnvLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolumDeSignum\\PackageEnvLoader\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-master",
            "dev-dev": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
