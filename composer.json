{
    "name": "solumdesignum/package-env-loader",
    "description": "Framework agnostic environment loader that abstracts PHP dotEnv for faster env loading.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskars Germovs",
            "email": "oskars_germovs@inbox.lv",
            "homepage": "https://solum-designum.eu"
        }
    ],
    "homepage": "https://github.com/solumdesignum/package-env-loader",
    "keywords": [
        "packageEnvLoader",
        "pacakge",
        "loader",
        "env",
        "dotenv",
        "environment"
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "vlucas/phpdotenv": "^v5.5.0"
    },
    "require-dev": {
        "php": "^8.0|^8.1|^8.2",
        "vlucas/phpdotenv": "^v5.5.0",
        "phpunit/phpunit": "~8|~9|~10",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6|~7",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SolumDeSignum\\PackageEnvLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolumDeSignum\\PackageEnvLoader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
